<Window x:Class="Vizualizer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Vizualizer"
        mc:Ignorable="d"
        Title="Sound vizualizer" Height="450" Width="800" WindowState="Maximized" WindowStyle="None">
    <Grid Background="{DynamicResource MainBackGroundColor}" Name="MGrid">
        <ContentPresenter x:Name="ContentPresenter"/>
    </Grid>
</Window>
